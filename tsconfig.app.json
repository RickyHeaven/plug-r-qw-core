{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": [
    "env.d.ts",
    "./src/**/*",
    "./examples/**/*"
  ],
  "exclude": [
    "./src/components/md.ts",
    "./src/methods/index.ts"
  ],
  "compilerOptions": {
    "allowJs": true,
    "baseUrl": ".",
    "composite": true,
    "moduleResolution": "node",
    "noImplicitAny": false,
    "paths": {
      "@/*": [
        "./examples/*"
      ]
    }
  }
}
