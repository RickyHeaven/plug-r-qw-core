{
  "name": "@zhangqingcq/plug-r-qw",
  "version": "2.0.28",
  "description": "A JS lib base on Vue@3.x and view-ui-plus, you can achieve some complex functions with simple code after install this lib.",
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "files": [
    "lib",
    "src",
    "types"
  ],
  "main": "./lib/plugRQw.umd.cjs",
  "module": "./lib/plugRQw.js",
  "types": "types/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/plugRQw.js",
      "require": "./lib/plugRQw.umd.cjs",
      "types": "./types/index.d.ts"
    },
    "./lib/plugRQw.min.css": "./lib/plugRQw.min.css",
    "./lib/**/*.js": "./lib/**/*.js",
    "./*": "./*"
  },
  "scripts": {
    "dev": "vite",
    "preview": "vite preview",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "clean": "gulp types",
    "types": "vue-tsc --declaration --emitDeclarationOnly -p tsconfig.app.declaration.json --composite false",
    "node-serve": "node node-serve/index.cjs",
    "node-serve-dev": "nodemon node-serve/index.cjs",
    "lib": "vue-tsc --noEmit --skipLibCheck && vite build --mode lib && gulp lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RickyHeaven/plug-r-qw-core.git"
  },
  "keywords": [
    "vue",
    "view-ui-plus",
    "vite",
    "typescript",
    "vue-router",
    "pinia",
    "sweetalert",
    "lodash",
    "axios",
    "echarts",
    "moment",
    "wangeditor",
    "xss",
    "js-cookie",
    "iview-area",
    "ar-cascader",
    "vue-json-viewer",
    "qw",
    "ricky",
    "zhangqingcq"
  ],
  "author": "RickyHeaven",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/RickyHeaven/plug-r-qw-core/issues"
  },
  "homepage": "https://github.com/RickyHeaven/plug-r-qw-core#readme",
  "dependencies": {
    "@amap/amap-jsapi-loader": "^1.0.1",
    "@types/lodash-es": "^4.17.8",
    "@wangeditor/editor": "^5.1.23",
    "@wangeditor/editor-for-vue": "^5.1.12",
    "ar-cascader": "^2.0.0",
    "axios": "^1.4.0",
    "deepmerge": "^4.3.1",
    "echarts": "^5.4.2",
    "js-cookie": "3.0.5",
    "lodash-es": "^4.17.21",
    "moment": "^2.29.4",
    "sweetalert": "^2.1.2",
    "view-ui-plus": "^1.3.14",
    "vue": "^3.3.4",
    "vue-json-viewer": "^3.0.4"
  },
  "devDependencies": {
    "@jiaminghi/data-view": "^2.10.0",
    "@tsconfig/node18": "^2.0.1",
    "@types/markdown-it": "^12.2.3",
    "@types/node": "^18.16.17",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "@vue/tsconfig": "^0.4.0",
    "@zhangqingcq/view-ui-plus-resolver": "^0.0.1",
    "china-map-data": "^1.0.4",
    "del": "^6.1.1",
    "express": "^4.18.2",
    "github-markdown-css": "^5.2.0",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^8.0.0",
    "gulp-clean-css": "^4.3.0",
    "gulp-less": "^5.0.0",
    "gulp-rename": "^2.0.0",
    "less": "^4.2.0",
    "markdown-it": "^13.0.1",
    "markdown-it-anchor": "^8.6.7",
    "multer": "1.4.5-lts.1",
    "nodemon": "^2.0.22",
    "npm-run-all": "^4.1.5",
    "pinia": "^2.1.3",
    "prettier": "^3.0.0",
    "typescript": "~5.0.4",
    "unplugin-auto-import": "^0.16.6",
    "unplugin-vue-components": "^0.25.1",
    "vite": "^4.3.9",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.2.2",
    "vue-tsc": "^1.6.5"
  },
  "engines": {
    "node": ">=18.16.0",
    "npm": ">=9.5.1",
    "pnpm": ">=8.6.0"
  }
}
